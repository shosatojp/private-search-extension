{
    "manifest_version": 3,
    "name": "private-search",
    "version": "0.0.1",
    "author": "shosato",
    "host_permissions": [
        "*://*/*"
    ],
    "permissions": [
        "declarativeNetRequest",
        "declarativeNetRequestWithHostAccess",
        "declarativeNetRequestFeedback"
    ],
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    }
}
